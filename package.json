{
  "name": "sample-node-api",
  "version": "1.0.0",
  "description": "> RESTful api with Domain Driven Design",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "gc": "git-cz",
    "start": "NODE_PATH=. node cluster.js",
    "start:dev": "NODE_PATH=. nodemon index.js",
    "test": "nyc npm run test:unit",
    "test:unit": "npm run db:refresh-test && cross-env NODE_PATH=. NODE_ENV=test mocha --config test/unit.mocharc.js",
    "db:reset": "cross-env NODE_PATH=. && echo $NODE_PATH && yarn db:refresh && yarn seed",
    "db:refresh": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "db:refresh-test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env test",
    "seed": "cross-env NODE_ENV=development NODE_PATH=. sequelize db:seed:all --seeders-path=./src/infra/sequelize/seeders/development",
    "seed:test": "cross-env NODE_ENV=development sequelize db:seed:all --env test --seeders-path=./src/infra/sequelize/seeders/development",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "lint:staged": "lint-staged",
    "fresh-install": "rm -rf node_modules && npm cache clean --force && npm install",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "author": "sample-node-api",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "awilix": "^8.0.0",
    "awilix-express": "^8.0.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "celebrate": "^15.0.1",
    "chai": "^4.3.7",
    "chai-change": "^2.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dirty-chai": "^2.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "faker": "^6.6.6",
    "http-status": "^1.6.2",
    "inquirer": "^9.1.4",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^8.1.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "morgan": "^1.10.0",
    "node-pre-gyp": "^0.17.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^5.2.2",
    "ramda": "^0.28.0",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.5.2",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "tcomb": "^3.2.29",
    "tedious": "^15.1.2",
    "uuid": "^8.3.2",
    "winston": "^3.8.2"
  }
}
